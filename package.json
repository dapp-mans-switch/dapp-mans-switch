    "lit-html": "^1.4.1",
    "mathjs": "^9.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shamir": "^0.7.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1"
  }
}
